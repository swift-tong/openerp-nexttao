<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- mymenu Form View -->
        <record id="mymenu_form" model="ir.ui.view">
            <field name="name">mymenu.mymenu.form</field>
            <field name="model">mymenu.mymenu</field>
            <field name="arch" type="xml">
                <form string="mymenu" version="1.0">
                    <field name="x_daterequired"/>
                    <field name="x_rush"/>
                </form>
            </field>
        </record>

        <!-- mymenu Tree View -->
        <record model="ir.ui.view" id="mymenu_tree">
            <field name="name">mymenu.mymenu.tree</field>
            <field name="model">mymenu.mymenu</field>
            <field name="arch" type="xml">
                <tree string="mymenu" editable="bottom">
                    <field name="sequence" widget="handle"/>
                    <field name="x_daterequired"/>
                    <field name="x_rush"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>